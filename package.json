{
  "name": "routing-controllers",
  "private": false,
  "version": "0.9.0",
  "description": "Forked package from TypeStack. Create structured, declarative and beautifully organized class-based controllers with heavy decorators usage for Express / Koa using TypeScript.",
  "author": "TypeStack and Mendix contributors",
  "license": "MIT",
  "sideEffects": false,
  "main": "./cjs/index.js",
  "module": "./esm2015/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mendix/routing-controllers.git"
  },
  "tags": [
    "express",
    "express.js",
    "typescript",
    "typescript-express",
    "typescript-express.js",
    "express-controllers",
    "controllers"
  ],
  "scripts": {
    "build": "npm run build:cjs",
    "build:clean": "rimraf build",
    "build:es2015": "tsc --project tsconfig.prod.esm2015.json",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "prettier:fix": "prettier --write \"**/*.{ts,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,md}\"",
    "lint:fix": "eslint --max-warnings 0 --fix --ext .ts src/",
    "lint:check": "eslint --max-warnings 0 --ext .ts src/",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --no-cache --coverage --verbose",
    "test:debug": "NODE_ENV= node --inspect=0.0.0.0:20001 ./node_modules/.bin/jest --colors --runInBand --no-coverage --watch"
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "glob": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "template-url": "^1.0.0"
  },
  "peerDependencies": {
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.3"
  },
  "devDependencies": {
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/class-validator": "^0.13.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/form-data": "^2.5.0",
    "@types/jest": "^27.0.2",
    "@types/koa": "^2.13.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "chakram": "^1.5.0",
    "cors": "^2.8.5",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.1.4",
    "jest": "^27.3.1",
    "kcors": "^2.2.2",
    "koa-convert": "^2.0.0",
    "koa-session": "^6.2.0",
    "koa-views": "^7.0.2",
    "multer": "^1.4.3",
    "mustache-express": "^1.3.2",
    "prettier": "^2.4.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "^6.1.3",
    "typedi": "~0.10.0",
    "typescript": "^4.4.4"
  },
  "optionalDependencies": {
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "multer": "^1.4.3",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.1.1",
    "koa-multer": "^1.0.2"
  }
}
